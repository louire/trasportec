{
  "expo": {
    "name": "Expo Uber",
    "description": "Uber: UI Clone with React Native & Expo",
    "scheme": "uberclone",
    "slug": "uberclone",
    "privacy": "public",
    "version": "0.0.1",
    "platforms": ["android", "ios"],
    "githubUrl": "https://github.com/calebnance/expo-uber",
    "orientation": "portrait",
    "primaryColor": "#000000",
    "icon": "src/assets/icon.png",
    "splash": {
      "backgroundColor": "#000000",
      "image": "src/assets/splash.png",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "versionCode": 1
    },
    "ios": {
      "buildNumber": "0.0.1",
      "supportsTablet": true
    }
  }
}
